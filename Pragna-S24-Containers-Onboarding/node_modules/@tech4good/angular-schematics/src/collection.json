{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "init": {
      "description": "Setup initial lab infrastructure files.",
      "factory": "./init/index#init",
      "schema": "./init/schema.json"
    },
    "module": {
      "description": "Generate a new module.",
      "factory": "./module/index#module",
      "schema": "./module/schema.json"
    },
    "container": {
      "description": "Generate a new container.",
      "factory": "./container/index#container",
      "schema": "./container/schema.json"
    },
    "component": {
      "description": "Generate a new component.",
      "factory": "./component/index#component",
      "schema": "./component/schema.json"
    },
    "entity": {
      "description": "Generate a new entity.",
      "factory": "./entity/index#entity",
      "schema": "./entity/schema.json"
    }
  }
}
